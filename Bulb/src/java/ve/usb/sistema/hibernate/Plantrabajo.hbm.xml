<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ve.usb.sistema.hibernate.Plantrabajo" table="PLANTRABAJO">
      <!-- 0 -->
      <id name="idPlantrabajo" column="idPlantrabajo">
        <generator class="native"/>
      </id>

      <!-- 1 -->
      <property name="empresa" column="empresa" type="string" not-null="true"/>
      <!-- 2 -->
      <property name="tituloPasantia" column="tituloPasantia" type="string" not-null="true"/>
      <!-- 3 -->
      <property name="areaProyecto" column="areaProyecto" type="string" not-null="true"/>
      <!-- 4 -->
      <property name="resumenProyecto" column="resumenProyecto" type="string"/>
      <!-- 5 -->
      <property name="objetivosPasantias" column="objetivosPasantias" type="string" not-null="true"/>
      <!-- 6 -->
      <property name="fasesPasantia" column="fasesPasantia" type="string" not-null="true"/>
      <!-- 19 -->
      <property name="codigopt" column="codigopt" type="integer" not-null="true" unique="true"/>
      <!-- 20 -->
      <property name="codigoTI" column="codigoTI" type="string" not-null="true"/>
      <!-- 21 -->
      <property name="codigoTA" column="codigoTA" type="string"/>

    </class>
</hibernate-mapping>