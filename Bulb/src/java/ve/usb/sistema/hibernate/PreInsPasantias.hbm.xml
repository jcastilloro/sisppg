<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ve.usb.sistema.hibernate.PreInsPasantias" table="PREINSPASANTIAS">
      <!-- 0 -->
      <id name="idPreInsPasantias" column="idPreInsPasantias">
        <generator class="native"/>
      </id>

      <!-- 1 -->
      <property name="ep" column="ep" type="integer" not-null="true"/>
      <!-- 2 -->
      <property name="periodoPasantia" column="periodoPasantia" type="string" not-null="true"/>
      <!-- 3 -->
      <property name="estudiante" column="estudiante" type="string" not-null="true"/>
      <!-- 4 -->
      <property name="telef_hab" column="telef_hab" type="string" not-null="true"/>
      <!-- 5 -->
      <property name="otro_telef" column="otro_telef" type="string" not-null="true"/>
      <!-- 6 -->
      <property name="estado" column="estado" type="string"/>
      <!-- 7 -->
      <property name="region" column="region" type="string"/>
      <!-- 8 -->
      <property name="ciudad" column="ciudad" type="string"/>
      <!-- 9 -->
      <property name="tramiteCCTDS" column="tramiteCCTDS" type="boolean"/>
      <!-- 10 -->
      <property name="graduando" column="graduando" type="boolean"/>
      <!-- 11 -->
      <property name="ano" column="ano" type="integer" not-null="true"/>
      <!-- 12 -->
      <property name="correo" column="correo" type="string" not-null="true" unique="true"/>
      <!-- 13 -->
      <property name="direccion" column="direccion" type="string" not-null="true"/>
      <!-- 14 -->
      <property name="estado_civil" column="estado_civil" type="string" not-null="true"/>

    </class>
</hibernate-mapping>