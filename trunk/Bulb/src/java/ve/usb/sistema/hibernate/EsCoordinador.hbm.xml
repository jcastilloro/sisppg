<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ve.usb.sistema.hibernate.EsCoordinador" table="ESCOORDINADOR">
      <!-- 0 -->
      <id name="idEsCoordinador" column="idEsCoordinador">
        <generator class="native"/>
      </id>

      <!-- 1 -->
      <property name="codigoProfesor" column="codigoProfesor" type="string" not-null="true" unique="true"/>
      <!-- 2 -->
      <property name="coordinaCarrera" column="coordinaCarrera" type="string" not-null="true" unique="true"/>

    </class>
</hibernate-mapping>