<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ve.usb.sistema.hibernate.EstRealizaProy" table="ESTREALIZAPROY">
      <!-- 0 -->
      <id name="idEstRealizaProy" column="idEstRealizaProy">
        <generator class="native"/>
      </id>

      <!-- 1 -->
      <property name="carnetEstudiante" column="carnetEstudiante" type="string" not-null="true" unique="true"/>
      <!-- 2 -->
      <property name="codigoproy" column="codigoproy" type="integer" not-null="true"/>
      <!-- 3 -->
      <property name="tipoProy" column="tipoProy" type="integer"/>

    </class>
</hibernate-mapping>