<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="jc2s.sistppg.hibernate.PaisPreinscripcion" table="PAISPREINSCRIPCION">
      <!-- 0 -->
      <id name="idPaisPreinscripcion" column="idPaisPreinscripcion">
        <generator class="native"/>
      </id>

      <!-- 1 -->
      <many-to-one name="preinscripcion" column="preinscripcion" not-null="true" />      <!-- 2 -->
      <many-to-one name="pais" column="pais" not-null="true" />
    </class>
</hibernate-mapping>