<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="jc2s.sistppg.hibernate.Fase" table="FASE">
      <!-- 0 -->
      <id name="idFase" column="idFase">
        <generator class="native"/>
      </id>

      <!-- 1 -->
      <property name="objetivos_especificos" column="objetivos_especificos" type="text" not-null="true"/>
      <!-- 2 -->
      <set name="actividades" inverse="true" fetch="subselect"
        cascade="all, delete-orphan">
        <key column="fase"/>
        <one-to-many class="jc2s.sistppg.hibernate.ActividadFase"/>
      </set>
      <!-- 3 -->
      <many-to-one name="pasantia" column="pasantia" not-null="true" />
    </class>
</hibernate-mapping>