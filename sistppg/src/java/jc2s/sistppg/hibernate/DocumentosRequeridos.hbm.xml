<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="jc2s.sistppg.hibernate.DocumentosRequeridos" table="DOCUMENTOSREQUERIDOS">
      <!-- 0 -->
      <id name="idDocumentosRequeridos" column="idDocumentosRequeridos">
        <generator class="native"/>
      </id>

      <!-- 1 -->
      <many-to-one name="pasantia" column="pasantia" not-null="true" unique="true" />      <!-- 2 -->
      <property name="carta_aceptacion_tutor_academico" column="carta_aceptacion_tutor_academico" type="string"/>
      <!-- 3 -->
      <property name="carta_aceptacion_tutor_industrial" column="carta_aceptacion_tutor_industrial" type="string"/>
      <!-- 4 -->
      <property name="carta_retiro_tutor_academico" column="carta_retiro_tutor_academico" type="string"/>
      <!-- 5 -->
      <property name="carta_retiro_tutor_industrial" column="carta_retiro_tutor_industrial" type="string"/>
      <!-- 6 -->
      <property name="carta_retiro_estudiante" column="carta_retiro_estudiante" type="string"/>

    </class>
</hibernate-mapping>