<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="jc2s.sistppg.hibernate.AreaProfesor" table="AREAPROFESOR">
    <!-- 0 -->
    <id column="idAreaProfesor" name="idAreaProfesor">
      <generator class="native"/>
    </id>
    <!-- 1 -->
    <many-to-one column="profesor" name="profesor" not-null="true"/>
    <!-- 2 -->
    <many-to-one column="area" name="area" not-null="true"/>
  </class>
</hibernate-mapping>
