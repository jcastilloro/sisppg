<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="jc2s.sistppg.hibernate.ProrrogaEvaluacion" table="PRORROGAEVALUACION">
      <!-- 0 -->
      <id name="idProrrogaEvaluacion" column="idProrrogaEvaluacion">
        <generator class="native"/>
      </id>

      <!-- 1 -->
      <property name="fecha_propuesta" column="fecha_propuesta" type="date" not-null="true"/>
      <!-- 2 -->
      <many-to-one name="prorroga" column="prorroga" not-null="true" />
    </class>
</hibernate-mapping>