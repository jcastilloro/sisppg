<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="jc2s.sistppg.hibernate.Preinscripcion" table="PREINSCRIPCION">
      <!-- 0 -->
      <id name="idPreinscripcion" column="idPreinscripcion">
        <generator class="native"/>
      </id>

      <!-- 1 -->
      <property name="carta_postulacion" column="carta_postulacion" type="string"/>
      <!-- 2 -->
      <property name="tipo" column="tipo" type="integer" not-null="true"/>
      <!-- 3 -->
      <property name="curriculum" column="curriculum" type="string"/>
      <!-- 4 -->
      <property name="por_graduar" column="por_graduar" type="boolean"/>
      <!-- 5 -->
      <many-to-one name="estudiante" column="estudiante" not-null="true" unique="true" />      <!-- 6 -->
      <property name="created_at" column="created_at" type="timestamp" not-null="true"/>

    </class>
</hibernate-mapping>