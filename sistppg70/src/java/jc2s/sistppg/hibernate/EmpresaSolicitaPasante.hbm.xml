<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="jc2s.sistppg.hibernate.EmpresaSolicitaPasante" table="EMPRESASOLICITAPASANTE">
      <!-- 0 -->
      <id name="idEmpresaSolicitaPasante" column="idEmpresaSolicitaPasante">
        <generator class="native"/>
      </id>

      <!-- 1 -->
      <many-to-one name="empresa" column="empresa" not-null="true" />      <!-- 2 -->
      <many-to-one name="carrera" column="carrera" not-null="true" />      <!-- 3 -->
      <property name="created_at" column="created_at" type="timestamp" not-null="true"/>

    </class>
</hibernate-mapping>