<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="jc2s.sistppg.hibernate.Usuario" table="USUARIO">
    <!-- 0 -->
    <id column="idUsuario" name="idUsuario">
      <generator class="native"/>
    </id>
    <!-- 1 -->
    <property column="usbid" name="usbid" not-null="true" type="string" unique="true"/>
    <!-- 2 -->
    <property column="tipo_actor" name="tipo_actor" type="string"/>
  </class>
</hibernate-mapping>
